{
  "expo": {
    "name": "Lingucidity",
    "slug": "Lingucidity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": ["expo-apple-authentication"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Lingucidity"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Lingucidity"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": ["lingucidity", "lc6815058565214807"],
    "extra": {
      "eas": {
        "projectId": "06b0e20a-8880-4b0a-914f-b0b3e81cc73b"
      }
    }
  }
}
